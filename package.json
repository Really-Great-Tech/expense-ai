{
  "name": "expense-ai",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:ci": "jest --coverage --ci --watchAll=false",
    "typeorm": "typeorm-ts-node-commonjs -d ./src/config/database.ts",
    "schema:sync": "npm run typeorm schema:sync",
    "migration:generate": "npm run typeorm migration:generate",
    "migration:create": "npm run typeorm migration:create",
    "migration:run": "npm run build && npm run typeorm migration:run",
    "migration:run:dev": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:show": "npm run typeorm migration:show",
    "drop:database": "npm run typeorm schema:drop",
    "seed:countries": "npm run migration:run -- src/migrations/1736504407000-SeedCountryPolicies.ts",
    "seed:countries:rollback": "npm run migration:revert -- src/migrations/1736504407000-SeedCountryPolicies.ts",
    "seed:validate": "node -e \"const { Logger } = require('@nestjs/common'); new Logger('SeedValidate').log('Use API endpoint: GET /country-policies/health for validation');\"",
    "docker:build": "docker compose build",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "security:audit": "npm audit",
    "prepare": "if [ -d '.git' ]; then husky; fi"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.857.0",
    "@aws-sdk/client-rds": "^3.913.0",
    "@aws-sdk/client-s3": "^3.884.0",
    "@aws-sdk/client-textract": "^3.848.0",
    "@aws-sdk/rds-signer": "^3.913.0",
    "@aws-sdk/types": "^3.840.0",
    "@casl/ability": "6.7.3",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/common": "11.1.5",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.1.5",
    "@nestjs/microservices": "^11.1.5",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/mapped-types": "2.1.0",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "11.1.5",
    "@nestjs/schedule": "6.0.0",
    "@nestjs/swagger": "11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "11.0.0",
    "amqplib": "^0.10.3",
    "amqp-connection-manager": "^4.1.14",
    "bcryptjs": "3.0.2",
    "bullmq": "^5.28.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "dotenv": "16.4.7",
    "helmet": "8.0.0",
    "ioredis": "^5.8.2",
    "langfuse": "^3.38.4",
    "llamaindex": "^0.11.28",
    "mime-types": "^2.1.35",
    "multer": "^2.0.2",
    "mysql2": "^3.14.5",
    "p-limit": "^3.1.0",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "pdf-lib": "^1.17.1",
    "reflect-metadata": "0.2.2",
    "rimraf": "6.0.1",
    "rxjs": "7.8.2",
    "swagger-ui-express": "5.0.1",
    "typeorm": "0.3.25",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@nestjs/cli": "11.0.10",
    "@nestjs/schematics": "11.0.7",
    "@nestjs/testing": "11.1.5",
    "@types/bcryptjs": "3.0.0",
    "@types/cron": "2.4.3",
    "@types/eslint": "9.6.1",
    "@types/eslint-config-prettier": "6.11.3",
    "@types/eslint-plugin-prettier": "3.1.3",
    "@types/express": "5.0.3",
    "@types/jest": "29.5.14",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^2.0.0",
    "@types/mysql": "^2.15.27",
    "@types/node": "22.10.5",
    "@types/passport": "1.0.17",
    "@types/passport-jwt": "4.0.1",
    "@types/passport-local": "1.0.38",
    "@types/prettier": "3.0.0",
    "@types/rimraf": "4.0.5",
    "@types/supertest": "6.0.3",
    "@types/swagger-ui-express": "4.1.8",
    "@typescript-eslint/eslint-plugin": "8.10.0",
    "@typescript-eslint/parser": "8.10.0",
    "aws-sdk": "^2.1692.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.3",
    "husky": "^9.1.7",
    "jest": "29.7.0",
    "prettier": "3.6.2",
    "supertest": "7.1.4",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.spec.ts",
      "!**/*.test.ts",
      "!**/index.ts",
      "!**/*.dto.ts",
      "!**/*.entity.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 20,
        "functions": 25,
        "lines": 30,
        "statements": 30
      }
    },
    "coverageDirectory": "../coverage",
    "coverageReporters": [
      "text",
      "text-summary",
      "lcov",
      "json-summary",
      "html"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1",
      "^@/(.*)$": "<rootDir>/$1",
      "^@auth/(.*)$": "<rootDir>/auth/$1",
      "^@users/(.*)$": "<rootDir>/users/$1",
      "^@tasks/(.*)$": "<rootDir>/tasks/$1",
      "^@config/(.*)$": "<rootDir>/config/$1"
    }
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
